{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "baseUrl": "..",
        "outDir": "./dist",
        "noEmit": false,
        "skipLibCheck": true,
        "composite": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}